{
  "name": "xpectum",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npm run cp:email-templates",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:dev-watch": "npm run build && tsc-watch --onSuccess \"node dist/src/main.js\" --onFailure \"echo There was a problem with the build!\" -p tsconfig.json",
    "build:prod": "rimraf dist && tsc && npm run cp:email-templates",
    "start:prod": "NODE_ENV=production node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "NODE_ENV= jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cp:email-templates": "cp -r src/email-templates dist/src/",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "typeorm-migration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert"
  },
